root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.json]
indent_size = 2

[node_modules/**]
indent_style = ignore
indent_size = ignore
end_of_line = ignore
charset = ignore
trim_trailing_whitespace = ignore
insert_final_newline = ignore
